{
  "name": "@mswjs/http-middleware",
  "version": "0.0.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "Artem Zakharchenko <kettanaito@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsc -w",
    "test": "jest",
    "lint": "eslint \"{examples,src,test}/**/*.ts\"",
    "clean": "rimraf ./lib",
    "build": "yarn clean && tsc",
    "example-build": "yarn build && cd examples && rimraf ./lib && tsc && cp -R public lib/examples/public",
    "example:basic": "yarn example-build && node examples/lib/examples/basic-server.js",
    "example:custom": "yarn example-build && node examples/lib/examples/custom-server.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "homepage": "https://mswjs.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/mswjs/http-middleware"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "msw": "^0.32.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "headers-utils": "^3.0.2"
  }
}
